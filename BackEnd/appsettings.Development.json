{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "MIGRATE": true,
  "DB_TYPE": "POSTGRES_LOCAL",
  "ConnectionStrings": {
    "POSTGRES_LOCAL": "User ID=postgres;Password=password;Server=127.0.0.1;Port=5432;Database=itlab-db;Integrated Security=true;"
  },
  "Authority": "http://localhost:5000",
  "NotifyType": "console",
  "UseRandomEventsGenerator": false,
  "RedisNotifierSettings": {
    "ConnectionString": "localhost",
    "ChannelName": "itlab_events"
  }
}
